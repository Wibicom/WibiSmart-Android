    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <WebView
                android:id="@+id/webviewTemperature"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Temperature"
                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                android:textColor="@color/background_floating_material_dark"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp" />

            <TextView
                android:id="@+id/weather_ti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="-"
                android:textSize="40dp"
                android:textAlignment="center"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <WebView
                android:id="@+id/webviewHumidity"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Humidity"
                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                android:textColor="@color/background_floating_material_dark"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp" />

            <TextView
                android:id="@+id/humidity_ti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="-"
                android:textSize="40dp"
                android:textAlignment="center"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <WebView
                android:id="@+id/webviewPressure"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Pressure"
                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                android:textColor="@color/background_floating_material_dark"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp" />

            <TextView
                android:id="@+id/pressure_ti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="-"
                android:textSize="40dp"
                android:textAlignment="center"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <WebView
                android:id="@+id/webviewAccelerometer"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="10dp"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Accelerometer"
                android:textAppearance="@style/TextAppearance.AppCompat.Title"
                android:textColor="@color/background_floating_material_dark"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp" />

            <TextView
                android:id="@+id/accelerometer_x_ti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="-"
                android:textSize="25dp"
                android:textAlignment="center"
                android:layout_marginTop="5dp" />

            <TextView
                android:id="@+id/accelerometer_y_ti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="-"
                android:textSize="25dp"
                android:textAlignment="center"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:id="@+id/accelerometer_z_ti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="-"
                android:textSize="25dp"
                android:textAlignment="center"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </android.support.v7.widget.CardView>



    </LinearLayout>

    </android.support.v4.widget.NestedScrollView>

